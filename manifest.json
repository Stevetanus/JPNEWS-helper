{
  "name": "JP News Helper TEST",
  "version": "0.1.21",
  "description": "A Chrome extension to help you learn Japanese by NHK News Easy.",
  "permissions": [
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "https://news.web.nhk/news/easy/*"
  ],
  "background": {
    "service_worker": "dist/background.js"
  },
  "commands": {
    "toggle-sidebar": {
      "suggested_key": {
        "default": "Ctrl+J",
        "mac": "MacCtrl+J"
      },
      "description": "Toggle the sidebar"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "https://news.web.nhk/news/easy/*"
      ],
      "js": [
        "dist/content.js"
      ]
    }
  ],
  "action": {
    "default_icon": {},
    "default_title": "JP News Helper",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
